{
  "expo": {
    "name": "LAKMobile",
    "slug": "LAKMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.laktaa.LAKMobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Access your photos to add them to a job listing",
          "cameraPermission": "Access your camera to take a photo for a job listing"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ea2d84c4-38d2-40cc-b8ae-e9590e1d6441"
      }
    }
  }
}
